{
	"name": "Clojure",
	"dockerComposeFile": [
		"docker-compose.yml"
	],
	"service": "clojure",
	"workspaceFolder": "..",
	"settings": {
		"terminal.integrated.profiles.linux": {
			"bash": {
				"path": "/bin/bash"
			}
		},
		"search.exclude": {
			"**/node_modules": true,
			"**/bower_components": true,
			"**/*.code-search": true,
			"**/env/**": true,
			"**/.vscode/**": true,
			"**/venv/**": true
		},
		"files.watcherExclude": {
			"**/target/**": true,
			"**/node_modules/*/**": true,
			"**/.hg/store/**": true,
			"**/.git/objects/**": true,
			"**/.git/subtree-cache/**": true,
			"**/node_modules/**": true,
			"**/env/**": true,
			"**/venv/**": true,
			"**/.vscode/**": true,
			"env-*": true
		},
		"indentRainbow.ignoreErrorLanguages": [
			"clojure",
		]
	},
	"extensions": [
		"mutantdino.resourcemonitor",
		"mechatroner.rainbow-csv",
		"donjayamanne.githistory",
		"felipecaputo.git-project-manager",
		"eamodio.gitlens",
		"oderwat.indent-rainbow",
		"ms-azuretools.vscode-docker",
		"ms-vscode.azurecli",
		"ms-azuretools.vscode-bicep",
		"wraith13.bracket-lens",
		"redhat.vscode-yaml",
		"coenraads.bracket-pair-colorizer",
		"aaron-bond.better-comments",
		"yzhang.markdown-all-in-one",
		"github.remotehub",
		"vsls-contrib.codetour",
		"mindaro-dev.file-downloader",
		"leocardoso94.clojure-snippets",
		"betterthantomorrow.calva",
		"avli.clojure",
		"borkdude.clj-kondo",
		"jamesnorton.continuum",
		"christian-kohler.path-intellisense"
	],
	"shutdownAction": "none",
	"postCreateCommand": "git config --global user.email 'no-reply@lyscm.github.com' && git config --global user.name 'lyscm'"
}